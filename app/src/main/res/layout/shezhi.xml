<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/stoolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>
    <com.sannmizu.nearby_alumni.denglu.MyOneLineView
        android:id="@+id/sone_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
    <com.sannmizu.nearby_alumni.denglu.MyOneLineView
        android:id="@+id/stwo_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
    <com.sannmizu.nearby_alumni.denglu.MyOneLineView
        android:id="@+id/sthree_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
    <com.sannmizu.nearby_alumni.denglu.MyOneLineView
        android:id="@+id/sfour_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
    <com.sannmizu.nearby_alumni.denglu.MyOneLineView
        android:id="@+id/sfive_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
    <Button
        android:id="@+id/stuichu"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="24dp"
        android:elevation="0dp"
        android:outlineProvider="none"
        android:background="@drawable/bg_login_submit"
        android:textSize="18sp"
        android:text="退出登陆"
        />
</LinearLayout>