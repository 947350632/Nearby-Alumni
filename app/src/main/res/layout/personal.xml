    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/ptoolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/pone_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/ptwo_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/pnight_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/peight_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/pthree_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/pfour_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/pfive_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/psix_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
        <com.sannmizu.nearby_alumni.denglu.MyOneLineView
            android:id="@+id/pseven_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </com.sannmizu.nearby_alumni.denglu.MyOneLineView>
</LinearLayout>
    </ScrollView>
</FrameLayout>